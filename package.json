    "lodash": "^4.17.21",
    "lottie-react": "^2.4.0",
    "lottie-web": "^5.12.2",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
